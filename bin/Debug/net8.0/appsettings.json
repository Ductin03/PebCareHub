{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Key": "aVerySecretKeyThatIsAtLeast16BytesLong", // Thay bằng khóa bí mật bạn đã tạo
    "Issuer": "MyApp", // Tên của nhà phát hành
    "Audience": "MyAppAudience" // Đối tượng nhận token
  }
}
